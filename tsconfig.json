{
  "compilerOptions": {
    "lib": [
      "es2020",
      "dom"
    ],
    "module": "commonjs",
    "target": "es2020",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "outDir": "dist",
    "rootDir": ".",
    "sourceMap": true,
    "resolveJsonModule": true
  }
}

